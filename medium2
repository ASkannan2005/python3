def main():
    num_values = int(input("Enter how many values you want to read: "))
    array = []

    for i in range(num_values):
        value = int(input(f"Enter the value of a[{i}]: "))
        array.append(value)

    array.sort()

    print("Array sorted in ascending order =", end=" ")
    for value in array:
        print(value, end=" ")

if __name__ == "__main__":
    main()
