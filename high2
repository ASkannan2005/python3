def count_subsets(arr):
    n = len(arr)
    return 2 ** n

def main():
    size = int(input("Enter array size: "))
    
    if size <= 0:
        print("Invalid input")
        return

    elements = list(map(int, input(f"Enter {size} elements: ").split()))

    result = count_subsets(elements)

    print(result)

if __name__ == "__main__":
    main()
